{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  extends: ["config:base"],
  dependencyDashboard: false,
  rangeStrategy: "update-lockfile",
  lockFileMaintenance: {
    enabled: true,
    automerge: true,
  },
  packageRules: [
    {
      groupName: "prettier and plugins",
      matchPackageNames: ["prettier", "*prettier-plugin-*"],
    },
  ],
}
