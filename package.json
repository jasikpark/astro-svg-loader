{
  "name": "@jasikpark/astro-svg-loader",
  "repository": {
    "url": "https://github.com/jasikpark/astro-svg-loader"
  },
  "license": "MIT",
  "version": "0.2.0",
  "type": "module",
  "exports": {
    ".": "./index.ts"
  },
  "files": [
    "src",
    "index.ts"
  ],
  "keywords": [
    "astro-component",
    "svg",
    "svg-loader"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:watch:ui": "vitest --ui",
    "lint": "eslint",
    "check:astro": "astro check",
    "check:ts": "tsc -p .",
    "check": "run-p check:*",
    "version": "changeset version && pnpm install --no-frozen-lockfile && pnpm format",
    "changeset": "changeset"
  },
  "devDependencies": {
    "@astrojs/check": "^0.3.1",
    "@changesets/cli": "^2.25.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.1",
    "@typescript-eslint/parser": "^6.9.1",
    "@vitest/ui": "^0.34.0",
    "astro": "^3.4.2",
    "eslint": "^8.52.0",
    "eslint-plugin-astro": "^0.31.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "fast-check": "^3.3.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "prettier-plugin-astro": "^0.12.2",
    "shiki": "^1.0.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.0"
  },
  "peerDependencies": {
    "astro": "^2.0.0 || ^3.0.0"
  },
  "engines": {
    "node": ">18",
    "pnpm": ">=7.9.5"
  },
  "packageManager": "pnpm@8.14.1",
  "dependencies": {
    "ultrahtml": "1.5.2"
  },
  "publishConfig": {
    "provenance": true
  }
}
